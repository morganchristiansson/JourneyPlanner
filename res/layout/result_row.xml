<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:a="http://schemas.android.com/apk/res/android"
	a:orientation="vertical"
    a:layout_width="wrap_content" a:layout_height="wrap_content">
    <LinearLayout a:layout_width="fill_parent" a:layout_height="wrap_content">
		<TextView a:id="@+id/start"
			a:layout_weight="1"
		    a:layout_width="wrap_content" a:layout_height="wrap_content"/>
		<TextView a:id="@+id/end"
			a:layout_weight="1"
			a:gravity="center_horizontal"
		    a:layout_width="wrap_content" a:layout_height="wrap_content"/>
		<TextView a:id="@+id/duration"
			a:layout_weight="1"
			a:gravity="right"
		    a:layout_width="wrap_content" a:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout a:id="@+id/types"
    	a:gravity="center"
	    a:layout_width="fill_parent" a:layout_height="wrap_content"/>
</LinearLayout>
